{
  "name": "clawd",
  "version": "1.0.0",
  "private": true,
  "workspaces": ["backend", "frontend"],
  "scripts": {
    "dev": "concurrently -n be,fe -c green,blue \"npm run dev -w backend\" \"npm run dev -w frontend\"",
    "build": "npm run build -w backend && npm run build -w frontend",
    "db:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=prisma/schema.prisma",
    "db:migrate:prod": "npx prisma migrate deploy --schema=prisma/schema.prisma",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "npx prisma studio --schema=prisma/schema.prisma"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "tsx": "^4.19.0",
    "prisma": "^6.3.0"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0"
  }
}
